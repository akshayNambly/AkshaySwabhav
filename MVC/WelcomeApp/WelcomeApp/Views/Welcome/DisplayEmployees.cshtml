@model IEnumerable<WelcomeApp.Models.ViewModel.DisplayEmpVM>

@{
    ViewBag.Title = "DisplayEmployees";
}

<h2>Employees List</h2>
<table>
    <tr>
        <th>Name</th>
        <th>Salary</th>
        <th>Date Of Joining</th>
    </tr>
    
        @foreach (var employee in Model)
        {
            <tr>
                <th>@employee.Name</th>
                <th>@employee.Salary</th>
                <th>@employee.DateOFJoining</th>
            </tr>
        }
</table>
